@{
    ViewBag.Title = "Design";
}

@section header
{
    <!-- Start Header -->
    <header id="header">
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <!-- Start Logo / Text -->
                    <a class="navbar-brand text-logo" href="@Url.Action("Index", "Home")"><i class="icon-signout"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</a>
                    <!-- End Logo / Text -->
                </div>
                <!-- Start Navigation -->
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav menu-right">
                        <li class="active"><a href="@Url.Action("Index","Home")#header">Главная</a></li>
                        <li class="dropdown">
                            <a href="#allServices" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Услуги</a>
                            <ul class="dropdown-menu">
                                <li><a href="@Url.Action("Fotobox","Home")">FOTOBOX</a></li>
                                <li><a href="@Url.Action("Brandbox","Home")">BRANDBOX</a></li>
                                <li><a href="@Url.Action("Instabox","Home")">INSTABOX</a></li>
                                <li><a href="@Url.Action("Socialmatic","Home")">SOCIALMATIC</a></li>
                                <li><a href="@Url.Action("Design","Home")">DESIGN</a></li>
                            </ul>
                        </li>
                        <li><a href="@Url.Action("Index", "Gallery")#photoGallery">Фотогалерея</a></li>
                        <li>@Html.ActionLink("Контакты", "Contact", "Home")</li>
                    </ul>
                </div>
                <!-- End Navigation  -->
            </div>
        </nav>
    </header>
    <!-- End Header -->
}



<div id="main-slider" class="carousel slide carousel-fade">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#main-slider" data-slide-to="0" class="active"></li>
        <li data-target="#main-slider" data-slide-to="1"></li>
        <li data-target="#main-slider" data-slide-to="2"></li>
        <li data-target="#main-slider" data-slide-to="3"></li>
        <li data-target="#main-slider" data-slide-to="4"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <!-- Start Slide 1  -->
        <div class="item cover Disign_slide1 active">
            <div class="carousel-caption">
            </div>
        </div>
        <!-- End Slide 1
        <!-- Start Slide 2  -->
        <div class="item cover Disign_slide2">
            <div class="carousel-caption">
            </div>
        </div>
        <!-- End Slide 2  -->
        <!-- Start Slide 3 -->
        <div class="item cover Disign_slide3">
            <div class="carousel-caption">
            </div>
        </div>
        <!-- End Slide 3  -->
        <div class="item cover Disign_slide4">
            <div class="carousel-caption">
            </div>
        </div>
        <div class="item cover Disign_slide5">
            <div class="carousel-caption">
            </div>
        </div>
    </div>

    <!-- Start Black Background Overlay  -->
    <div class="overlay-bg"></div>
    <!-- End Background Overlay  -->
    <!-- Start Navigations  -->
    <a class="left carousel-control" href="#main-slider" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
    <a class="right carousel-control" href="#main-slider" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
    <!-- End Navigations  -->
</div>


<section id="about" class="section sectionsmall">
    <div class="container">
        <div class="row">
            <!-- Start Headline -->
            <div class="col-lg-12 headline">
                <h1>DESIGN </h1>
            </div>
            <!-- End Headline  -->
        </div>
    </div>
</section>

